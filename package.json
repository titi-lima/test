{
  "name": "boilerplate-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.9.0",
    "zod": "^3.21.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.67"
  },
  "scripts": {
    "build": "swc src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "yarn build && node dist/server.js",
    "dev": "./node_modules/.bin/ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "prod": "yarn prisma migrate deploy && yarn dev",
    "migration": "cross-env DATABASE_HOST='localhost' yarn prisma migrate dev",
    "migration:reset": "cross-env DATABASE_HOST='localhost' yarn prisma migrate reset --force",
    "studio": "cross-env DATABASE_HOST='localhost' yarn prisma studio",
    "generate": "yarn prisma generate",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test:ci": "cross-env DATABASE_URL='postgres://postgres:postgres@localhost:5432/postgres' yarn prisma db push && cross-env JWT_REFRESH_SECRET=CITi JWT_ACCESS_SECRET=CITi DATABASE_URL='postgres://postgres:postgres@localhost:5432/postgres' jest --runInBand --ci",
    "test": "cross-env DATABASE_URL='postgres://postgres:docker@localhost:5433/boilerplate-test' yarn prisma db push && cross-env DATABASE_URL='postgres://postgres:docker@localhost:5433/boilerplate-test' jest --runInBand"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.26",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^16.9.4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^29.6.1",
    "lint-staged": ">=10",
    "prisma": "^5.0.0",
    "supertest": "^6.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^5.1.6"
  }
}
